#!/usr/bin/env php
<?php

require __DIR__.'/vendor/autoload.php';

use Symfony\Component\Console\Application;

$application = new Application();

$application->add(new \Rancherize\Commands\StartCommand());

// ... register commands

try{

	$returnCode = $application->run();

} catch(Exception $e) {

	return $e->getCode();

}

return $returnCode;
